void Print(int n){
	int dp[n][n];
	memset(dp,0,sizeof(dp));
	for(int i=0;i<n;i++){
		dp[i][0] = 1;
		dp[i][i] = 1;
	}
	for(int i=2;i<n;i++){
		for(int j=1;j<n-1;j++){
			dp[i][j] = dp[i-1][j-1] + dp[i-1][j];
		}
	}
	int space = n;
	for(int i=0;i<n;i++){
		space--;
		for(int j=0;j<space;j++){
			cout<<" ";
		}
		for(int j=0;j<i+1;j++){
			cout<<dp[i][j]<<" ";
		}
		
		cout<<endl;
	}
}
